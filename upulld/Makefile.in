SRCS	 = rpc.c main.c 
OBJS	 = $(SRCS:%.c=%.o)

LDLIBS  += $(UCONN_LDLIBS) $(TCLX_LIB_SPEC) $(TCL_LIB_SPEC) $(TCL_LIBS)
LDFLAGS += $(UCONN_LDFLAGS)
LDDEPS  += $(UCONN_LDDEPS)

CPPFLAGS += $(UCONN_CPPFLAGS)

.KEEP_STATE:

client: upulld

upulld: $(OBJS) $(LDDEPS)
	$(LINK.c) -o $@ $(OBJS) $(LDLIBS)

rpc.o : bootstrap.h

clean::
	$(RM) upulld bootstrap.h

install-client::
	$(INSTALL_PROGRAM) upulld $(UNAMEIT_SBIN)/exe/upulld
