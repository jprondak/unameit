SRCS = main.c 
OBJS = $(SRCS:%.c=%.o)

LDLIBS += -lradix -laddress $(TCLX_LIB_SPEC) $(TCL_LIB_SPEC) $(TCL_LIBS)
LDFLAGS += -L$(TOP)/libradix -L$(TOP)/libaddress

TCL_INC = atomic.tcl
TCL_DEP = $(TCL_INC:%=$(TOP)/library/%)

.KEEP_STATE:

server: uparsedb

bootstrap.h: bootstrap.tcl $(TCL_DEP)

main.o : bootstrap.h

uparsedb: $(OBJS)
	$(LINK.c) -o $@ $(OBJS) $(LDLIBS)

clean::
	$(RM) uparsedb bootstrap.h


install-server::
	$(INSTALL_PROGRAM) uparsedb $(UNAMEIT_SBIN)/exe/uparsedb

